{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-snmp": "*",
        "ext-zip": "*",
        "divineomega/php-ssh-connection": "^1.3",
        "evilfreelancer/routeros-api-php": "^1.5",
        "freedsx/snmp": "^0.5.0",
        "fruitcake/laravel-cors": "^2.0",
        "graze/telnet-client": "^2.3",
        "guzzlehttp/guzzle": "^6.5.8",
        "hisorange/browser-detect": "^4.5",
        "laravel/framework": "^8.75",
        "laravel/passport": "10.4.2",
        "laravel/tinker": "^2.5",
        "laravolt/avatar": "^4.1",
        "laravolt/indonesia": "^0.34.0",
        "lcobucci/jwt": "4.0.4",
        "league/oauth2-server": "8.2.2",
        "madnest/madzipper": "^1.2",
        "mariuzzo/laravel-js-localization": "^1.10",
        "midtrans/midtrans-php": "^2.5",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "spatie/ssh": "^1.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
